@model Winn_BOA_Cash_Pro.Models.FedWire

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>FedWire</h4>
<hr />
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="../FedWires/_FedWiresPartialViews/From.cshtml" />
            <partial name="../FedWires/_FedWiresPartialViews/To.cshtml" />
            <partial name="../FedWires/_FedWiresPartialViews/Transaction.cshtml" />

            <input asp-for="CreatedBy" type="hidden" value="@User.Identity.Name" />
            <input asp-for="TransactionStatus" type="hidden" value="New" />

    <input type="button" value="Cancel" class="btn btn-secondary" onclick="window.history.go(-1); return false;" title="Your request will not be saved." />
    <button type="submit" class="btn btn-primary">
        Create & Close
    </button>
        </form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script type="text/javascript" src="@Url.Content("~/js/DropDown.js")" asp-append-version="true"></script>
}
